{
  "name": "New Tab Minimal",
  "description": "Bookmarks for HOMEPAGE",
  "version": "0.0.2.7",
  "manifest_version": 2,
  "background": {
      "scripts": ["js/bg.js"]
   },
  "icons": {
    "128": "icon_128.png"
  },
   "chrome_url_overrides": {
      "newtab": "start.html"
   },
   "options_page": "options.html",
   "permissions": [
    "unlimitedStorage",
    "contextMenus",
    "tabs",
    "<all_urls>"
  ]
}